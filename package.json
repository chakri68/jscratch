{
  "name": "jscratch",
  "displayName": "JScratch",
  "icon": "media/icon.png",
  "description": "In-Editor ETL Scratchpad",
  "publisher": "chakri68",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:datalab"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jscratch-explorer",
          "title": "JScratch",
          "icon": "$(beaker)"
        }
      ]
    },
    "views": {
      "jscratch-explorer": [
        {
          "id": "jscratch.pipelineView",
          "name": "Active Session"
        },
        {
          "id": "jscratch.historyView",
          "name": "History"
        }
      ]
    },
    "commands": [
      {
        "command": "jscratch.newSession",
        "title": "JScratch: New Session",
        "icon": "$(add)"
      },
      {
        "command": "jscratch.loadSession",
        "title": "Load Session"
      },
      {
        "command": "jscratch.deleteSession",
        "title": "Delete Session",
        "icon": "$(trash)"
      },
      {
        "command": "jscratch.createInput",
        "title": "New Input",
        "icon": "$(file-add)"
      },
      {
        "command": "jscratch.addTransform",
        "title": "Add Transformation",
        "icon": "$(beaker)"
      },
      {
        "command": "jscratch.runTransform",
        "title": "Run Transformation",
        "icon": "$(play)"
      },
      {
        "command": "jscratch.exportFile",
        "title": "Export File",
        "icon": "$(arrow-circle-down)"
      },
      {
        "command": "jscratch.clearHistory",
        "title": "Clear History",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "jscratch.newSession",
          "when": "view == jscratch.pipelineView",
          "group": "navigation"
        },
        {
          "command": "jscratch.createInput",
          "when": "view == jscratch.pipelineView",
          "group": "navigation"
        },
        {
          "command": "jscratch.clearHistory",
          "when": "view == jscratch.historyView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "jscratch.addTransform",
          "when": "view == jscratch.pipelineView && viewItem == input",
          "group": "inline"
        },
        {
          "command": "jscratch.addTransform",
          "when": "view == jscratch.pipelineView && viewItem == output",
          "group": "inline"
        },
        {
          "command": "jscratch.exportFile",
          "when": "view == jscratch.pipelineView && (viewItem == input || viewItem == output)",
          "group": "inline"
        },
        {
          "command": "jscratch.deleteSession",
          "when": "view == jscratch.historyView && viewItem == session",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  },
  "dependencies": {
    "tsx": "^4.7.0"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
